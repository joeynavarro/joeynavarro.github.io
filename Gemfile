gem 'jekyll', '3.9.0' # Exact version to match github-pages requirement
gem 'jekyll-paginate'
gem 'jekyll-tagging'
gem 'jekyll-sitemap'
gem 'github-pages', '>= 200', '< 226'
gem 'public_suffix', '~> 3.0' # Ensure compatibility
gem 'liquid', '~> 4.0'
gem 'activesupport', '~> 5.2.8' # Adjust to be compatible with jekyll
gem 'i18n', '>= 0.9.5', '< 2' # Specify upper limit to maintain compatibility
gem 'wdm', '>= 0.1.0' if Gem.win_platform?
